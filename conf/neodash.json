{
  "title": "US Covid 19",
  "version": "2.2",
  "settings": {
    "pagenumber": 2,
    "editable": true,
    "fullscreenEnabled": false,
    "parameters": {}
  },
  "pages": [
    {
      "title": "Meta & Stats",
      "reports": [
        {
          "title": "Hi there ðŸ‘‹",
          "query": "**This is US Daily Covid 19 dashboard!** \n \n",
          "width": 3,
          "height": 2,
          "x": 0,
          "y": 0,
          "type": "text",
          "selection": {},
          "settings": {}
        },
        {
          "title": "Meta Graph",
          "query": "CALL apoc.meta.graph\n\n",
          "width": 7,
          "height": 2,
          "x": 3,
          "y": 0,
          "type": "graph",
          "selection": {
            "Airport": "(label)",
            "County": "name",
            "DailyC19": "name"
          },
          "settings": {
            "nodePositions": {}
          }
        },
        {
          "title": "Data statistics",
          "query": "Call apoc.meta.data();\n\n\n",
          "width": 10,
          "height": 3,
          "x": 0,
          "y": 2,
          "type": "table",
          "selection": {},
          "settings": {}
        }
      ]
    },
    {
      "title": "California Cases",
      "reports": [
        {
          "title": "On March 31, 2022",
          "query": "MATCH (d:DailyC19) WHERE d.date = '2020-03-31' AND d.state = \"California\" RETURN d.county AS county, d.cases AS cases, d.deaths AS deaths\n\n\n",
          "width": 3,
          "height": 5,
          "x": 0,
          "y": 0,
          "type": "table",
          "selection": {},
          "settings": {}
        },
        {
          "title": "California Counties",
          "query": "MATCH (c:County) WHERE c.state_name = \"California\" RETURN c",
          "width": 7,
          "height": 5,
          "x": 3,
          "y": 0,
          "type": "map",
          "selection": {
            "County": "county_full"
          },
          "settings": {}
        }
      ]
    },
    {
      "title": "Covid cases",
      "reports": [
        {
          "title": "Snohomish County, WA vs Los Angeles County, CA",
          "query": "MATCH (d1:DailyC19 {fips: \"53061\"})\n    WHERE d1.date > \"2020-03-31\"\nWITH d1 MATCH (d2:DailyC19 {fips: \"06037\", date: d1.date})\nRETURN DATE(d1.date) AS date, d1.cases, d1.deaths, d2.cases, d2.deaths LIMIT 90",
          "width": 12,
          "height": 4,
          "x": 0,
          "y": 0,
          "type": "line",
          "selection": {
            "x": "date",
            "value": [
              "d2.cases",
              "d1.cases"
            ]
          },
          "settings": {
            "nodePositions": {},
            "legend": true,
            "colors": "accent"
          }
        }
      ]
    },
    {
      "title": "Connected Counties",
      "reports": [
        {
          "title": "Snohomish's connected counties via air routes",
          "query": "MATCH (d1:County {county_fips: \"53061\"})-[r1:C2A]-(a1:Airport)-[r2:A2A]-(a2:Airport)-[r3:C2A]-(d2:County)\n    RETURN d1, r1, a1, r2, a2, r3, d2\n",
          "width": 10,
          "height": 4,
          "x": 0,
          "y": 0,
          "type": "graph",
          "selection": {
            "County": "county_full",
            "Airport": "municipality"
          },
          "settings": {
            "nodePositions": {}
          }
        }
      ]
    },
    {
      "title": "Connected Counties",
      "reports": [
        {
          "title": "Snohomish's air routes to Los Angeles and Orange Counties",
          "query": "\nMATCH (d1:County {county_fips: \"53061\"})-[r1:C2A]-(a1:Airport)-[r2:A2A]-(a2:Airport)-[r3:C2A]-(d2:County)\nWHERE d2.county_fips IN [\"06037\", \"06059\"]\nRETURN d1, r1, a1, r2, a2, r3, d2\n\n\n\n\n\n\n\n",
          "width": 10,
          "height": 5,
          "x": 0,
          "y": 0,
          "type": "map",
          "selection": {
            "County": "county_full",
            "Airport": "name"
          },
          "settings": {
            "nodePositions": {}
          }
        }
      ]
    }
  ],
  "parameters": {},
  "extensions": {}
}